{
  "refresh_interval": "10s",
  "services": [
    {
      "url": "https://api.github.com/repos/microsoft/vscode",
      "title": "🆚 VS Code",
      "headers": ["X-Test-Header: {TEST_API_KEY}"],
      "refresh_interval": "10s",
      "selectors": [
        {
          "name": "⭐ Stars",
          "value": "stargazers_count"
        },
        {
          "name": "🍴 Forks",
          "value": "forks_count"
        },
        {
          "name": "📝 Language",
          "value": "language"
        },
        {
          "name": "📊 Size",
          "value": "size"
        }
      ]
    },
    {
      "url": "https://api.github.com/repos/golang/go",
      "title": "🐹 Go Language",
      "selectors": [
        {
          "name": "⭐ Stars",
          "value": "stargazers_count"
        },
        {
          "name": "🐛 Issues",
          "value": "open_issues_count"
        },
        {
          "name": "📅 Updated",
          "value": "updated_at"
        }
      ]
    },
    {
